<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">
    <input>
        <net-file value="2LaneNetwork.net.xml"/>
        <route-files value="2LaneRoute.rou.xml"/>
    </input>
    <routing>
        <device.rerouting.probability value="1.0"/> <!-- Enable rerouting for all vehicles -->
        <device.rerouting.period value="10"/> <!-- Vehicles reroute every 10 seconds -->
    </routing>
    <gui-settings-file value="view.settings.xml"/>
</configuration>
